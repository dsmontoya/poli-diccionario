<h1>Listing profesores</h1>

<table>
  <thead>
    <tr>
      <th>Nombres</th>
      <th>Apellidos</th>
      <th>Url</th>
      <th>Email</th>
      <th>Sexo</th>
      <th>Token</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profesores.each do |profesor| %>
      <tr>
        <td><%= profesor.nombres %></td>
        <td><%= profesor.apellidos %></td>
        <td><%= profesor.url %></td>
        <td><%= profesor.email %></td>
        <td><%= profesor.sexo %></td>
        <td><%= profesor.token %></td>
        <td><%= link_to 'Show', profesor %></td>
        <td><%= link_to 'Edit', edit_profesor_path(profesor) %></td>
        <td><%= link_to 'Destroy', profesor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Profesor', new_profesor_path %>
